{
  "name": "@kyero/clients",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:account": "turbo run build --filter=@kyero/account",
    "build:docs": "turbo run build --filter=@kyero/docs",
    "build:frontend": "turbo run build --filter=@kyero/frontend",
    "build:join": "turbo run build --filter=@kyero/join",
    "build:scaffold": "turbo run build --filter=@kyero/scaffold",
    "dev:account": "turbo run dev --filter=@kyero/account",
    "dev:docs": "turbo run dev --filter=@kyero/docs",
    "dev:frontend": "turbo run dev --filter=@kyero/frontend",
    "dev:join": "turbo run dev --filter=@kyero/join",
    "dev:scaffold": "turbo run dev --filter=@kyero/scaffold",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start:account": "turbo run start --filter=@kyero/account",
    "start:docs": "turbo run start --filter=@kyero/docs",
    "start:frontend": "turbo run start --filter=@kyero/frontend",
    "start:join": "turbo run start --filter=@kyero/join",
    "start:scaffold": "turbo run start --filter=@kyero/scaffold",
    "e2e": "sh ./e2e.sh",
    "mocks": "mocks-server",
    "mocks-ci": "npm run mocks -- --no-plugins.inquirerCli.enabled",
    "test:unit": "turbo run test:unit",
    "prod-account": "turbo run build:prod --filter=@kyero/account",
    "prod-frontend": "turbo run build:prod --filter=@kyero/frontend",
    "prod-docs": "npm run build:docs",
    "prod-join": "npm run build:join",
    "heroku-postbuild": "if [ $AGENT_ADMIN_APP ]; then npm run prod-account; elif [ $DOCS_APP ]; then npm run prod-docs; elif [ $JOIN_APP ]; then npm run prod-join; else echo no environment detected, please set AGENT_ADMIN_APP, DOCS_APP or JOIN_APP; fi"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@mocks-server/admin-api-client": "^7.0.0",
    "@mocks-server/main": "^4.0.0",
    "@playwright/test": "^1.35.1",
    "@sentry/cli": "^2.13.0",
    "@types/react-google-recaptcha": "^2.1.5",
    "autoprefixer": "^10.4.2",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.6.0",
    "cssnano": "^5.1.15",
    "dotenv-cli": "^7.1.0",
    "eslint-config-custom": "*",
    "playwright-watch": "^1.3.23",
    "postcss": "^8.4.6",
    "postcss-hash": "^3.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "start-server-and-test": "^1.15.4",
    "tailwindcss": "^3.0.23",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "npm@8.11.0",
  "dependencies": {
    "lodash": "^4.17.21",
    "react-google-recaptcha": "^2.1.0",
    "remix-utils": "^6.0.0"
  }
}